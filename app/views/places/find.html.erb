<% if @placeable.user? %>
  <%= render 'layouts/sidenav' %>
  <%= render partial: 'searchbox', locals: { message: "スポットの名称やキーワードで検索できます。気になるスポットを見つけたらお気に入りに追加しておきましょう。" } %>
<% else %>
  <%= render 'layouts/sidenav_itinerary' %>
  <%= render partial: 'searchbox', locals: { message: "スポットの名称やキーワードで検索できます。行きたい場所を見つけたら旅のスポットリストに追加しましょう。" } %>
<% end %>
