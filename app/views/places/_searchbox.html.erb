<main class="main-content position-relative">
  <h5 class="page-heading">スポットを探す</h5>
  <div class="container-fluid py-3 py-sm-4 px-0 min-vh-100">
    <div class="max-width-850 mx-auto">
      <div class="card mb-2 p-3 p-sm-4 text-dark">
        <div class="row">
          <div class="col">
            <div class="search-form-with-icon">
              <input type="search" id="searchbox_text_input" placeholder="スカイツリー、東京駅 カフェ" autocomplete="false">
              <input type="hidden" id="user_id" value="<%= current_user.id %>">
              <input type="hidden" id="itinerary_id" value="<%= @itinerary.id if @itinerary %>">
            </div>
          </div>
          <div class="col-md-5 mt-2 mt-md-0 ps-md-1 d-flex align-items-center">
            <p class="fs-px-13 lh-base"><%= message %></p>
          </div>
        </div>
        <div class="mt-3">
          <div id="map"></div>
        </div>
      </div>
    </div>
  </div>
</main>
